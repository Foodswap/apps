{
  "name": "foodswap",
  "description": "Front app of FoodSwap",
  "version": "0.0.2",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "clean": "rm -rf dist",
    "clean:all": "rm -rf dist node_modules yarn.lock package-lock.json",
    "start-fake-api": "json-server --watch db.json",
    "start:prod": "http-server -p 80 -s dist",
    "commit": "cz",
    "test": "jest",
    "test:coverage": "jest --collect-coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.12.13",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "git-cz": "^4.7.6",
    "html-webpack-plugin": "^5.3.1",
    "http-server": "^0.12.3",
    "jest": "^26.6.3",
    "json-server": "^0.16.3",
    "mini-css-extract-plugin": "^1.3.9",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "axios": "^0.21.1",
    "browser-image-compression": "^1.0.14",
    "commitizen": "^4.2.3",
    "date-fns": "^2.19.0",
    "dotenv-webpack": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-context": "^1.1.2",
    "enzyme-context-react-router-4": "^2.1.0",
    "express": "^4.17.1",
    "html-loader": "^2.1.2",
    "install": "^0.13.0",
    "morgan": "^1.10.0",
    "npm": "^7.7.5",
    "prop-types": "^15.7.2",
    "proptypes": "^1.1.0",
    "rc-slider": "^9.7.2",
    "react": "^17.0.2",
    "react-autocomplete-input": "^1.0.17",
    "react-autosuggest": "^10.1.0",
    "react-content-loader": "^6.0.2",
    "react-dom": "^17.0.2",
    "react-highlight-words": "^0.17.0",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.3",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.3.0",
    "react-slick": "^0.28.1",
    "react-toastify": "^7.0.3",
    "redux": "^4.0.5",
    "semantic-ui-react": "^2.0.3",
    "slick-carousel": "^1.8.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "\\.(css|scss|less)$": "<rootDir>/tests/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "./tests/setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ]
  }
}
